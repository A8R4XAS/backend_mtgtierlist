{
  "settings": {
    "cors": {
      "allowedOrigins": "[\"https://frontend-mtgtierlist.onrender.com\"]"
    },
    "jwt": {
      "secret": "env(JWT_SECRET)",
      "refreshSecret": "env(JWT_REFRESH_SECRET)",
      "secretEncoding": "utf8"
    },
    "session": {
      "secret": "env(SESSION_SECRET)",
      "cookie": {
        "secure": true,
        "sameSite": "none"
      },
      "store": "@foal/typeorm"
    }
  },
  "database": {
    "synchronize": false
  }
}
