{
  "settings": {
    "session": {
      "store": "@foal/typeorm"
    },
    "disk": {
      "driver": "local",
      "local": {
        "directory": "./"
      }
    }
  },
  "database": {
    "type": "postgres",
    "host": "env(DB_HOST)",
    "port": "env(DB_PORT)",
    "username": "env(DB_USERNAME)",
    "password": "env(DB_PASSWORD)",
    "database": "env(DB_NAME)",
    "synchronize": "true"
  }
}